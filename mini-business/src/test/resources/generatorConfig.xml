<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 
	Mybatis生成Mapper配置文件
	1，cmd切换至项目根目录
	2，mvn mybatis-generator:generate -Dmybatis.generator.configurationFile=src/test/resources/generatorConfig.xml
 -->
<generatorConfiguration>
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<!-- <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" /> -->

		<commentGenerator>
			<property name="suppressDate" value="true"/>
			<property name="suppressAllComments" value="false"/>
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.0.1:3306/mini" userId="wuyu"
			password="wuyu">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.gongsi.mini.entities"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.gongsi.mini.dao" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


		<!--<table tableName="rule" delimitIdentifiers="true">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>
		</table>
		<table tableName="punish_event" delimitIdentifiers="true">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>
		</table>
		<table tableName="process_config" delimitIdentifiers="true">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>
		</table>-->
		<table tableName="cheat" delimitIdentifiers="true">
			<generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>
		</table>
		<!--<table tableName="appeal_award" delimitIdentifiers="true">-->
			<!--<generatedKey column="id" sqlStatement="MYSQL" identity="true" type="post"/>-->
		<!--</table>-->


	</context>
</generatorConfiguration>